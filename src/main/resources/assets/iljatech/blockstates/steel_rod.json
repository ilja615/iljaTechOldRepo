{
  "multipart": [
    {   "when": { "center": "true" },
      "apply": { "model": "iljatech:block/steel_rod_center", "uvlock": true }
    },
    {   "when": { "northeast": "true" },
      "apply": { "model": "iljatech:block/steel_rod", "uvlock": true }
    },
    {   "when": { "northwest": "true" },
      "apply": { "model": "iljatech:block/steel_rod", "y": 270, "uvlock": true }
    },
    {   "when": { "southeast": "true"},
      "apply": { "model": "iljatech:block/steel_rod", "y": 90, "uvlock": true }
    },
    {   "when": { "southwest": "true" },
      "apply": { "model": "iljatech:block/steel_rod", "y": 180, "uvlock": true }
    }
  ]
}