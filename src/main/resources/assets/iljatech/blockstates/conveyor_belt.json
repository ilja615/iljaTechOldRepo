{
  "multipart": [
    {   "when": { "axis": "x" },
      "apply": { "model": "iljatech:block/conveyor_belt_middle", "y": 90, "uvlock": true }
    },
    {   "when": { "axis": "z" },
      "apply": { "model": "iljatech:block/conveyor_belt_middle", "uvlock": true }
    },
    
    {   "when": { "axis": "x", "up": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_up_north", "uvlock": true }
    },
    {   "when": { "axis": "x", "down": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_down_south", "uvlock": true }
    },
    {   "when": { "axis": "x", "north": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_north_down", "uvlock": true }
    },
    {   "when": { "axis": "x", "south": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_south_up", "uvlock": true }
    },

    {   "when": { "axis": "z", "up": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_up_east", "uvlock": true }
    },
    {   "when": { "axis": "z", "down": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_down_west", "uvlock": true }
    },
    {   "when": { "axis": "z", "east": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_east_down", "uvlock": true }
    },
    {   "when": { "axis": "z", "west": "true", "rotationdirection": "clockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_west_up", "uvlock": true }
    },

    {   "when": { "axis": "x", "up": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_up_south", "uvlock": true }
    },
    {   "when": { "axis": "x", "down": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_down_north", "uvlock": true }
    },
    {   "when": { "axis": "x", "north": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_north_up", "uvlock": true }
    },
    {   "when": { "axis": "x", "south": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_south_down", "uvlock": true }
    },

    {   "when": { "axis": "z", "up": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_up_west", "uvlock": true }
    },
    {   "when": { "axis": "z", "down": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_down_east", "uvlock": true }
    },
    {   "when": { "axis": "z", "east": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_east_up", "uvlock": true }
    },
    {   "when": { "axis": "z", "west": "true", "rotationdirection": "counterclockwise" },
      "apply": { "model": "iljatech:block/conveyor_belt_west_down", "uvlock": true }
    }
  ]
}